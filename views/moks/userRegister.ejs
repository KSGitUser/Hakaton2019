
<%- include('../layouts/navbar.ejs')%>

<div class="container fishingform">
    <h3 class="title"> Регистрация </h3>
    <% if (msg) { %>
    <h2><%= msg %></h2>
    <% } %>
    <form action="/moks" method="POST" enctype="multipart/form-data" > 
        <input placeholder="Name" name="name"/><br>
        <input placeholder="Password" name="password"/><br><br>
        <input name="avatar" type="file" accept='image/png, image/jpeg'><br><br>
        <input type="email" name="email"  placeholder="Email"><br>
        <input type="number" name="phone" placeholder="Phone number">
        <button type="submit"> Принять </button>
    </form>  
</div>
<%- include('../layouts/footer.ejs')%>